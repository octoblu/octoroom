    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "jsdom": "^9.11.0",
    "mocha": "^3.0.2",
    "mocha-multi": "^0.10.0",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.2.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "webpack-dev-server": "^2.4.1"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
}
