{
  "name": "octoroom",
  "version": "7.0.10",
  "description": "octoroom",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "env NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "start": "webpack-dev-server --config webpack.config.dev.js --port 3000 --history-api-fallback",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "NODE_ENV=test mocha",
    "test:watch": "npm run test -- --watch --reporter mocha-multi --reporter-options spec=-,mocha-osx-reporter=-",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json"
  },
  "keywords": [
    "zooid",
    "zooid-app",
    "react",
    "babel",
    "webpack"
  ],
  "homepage": "https://github.com/octoblu/octoroom",
  "bugs": "https://github.com/octoblu/octoroom/issues",
  "author": {
    "name": "Octoblu Inc",
    "url": "https://github.com/octoblu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/octoroom.git"
  },
  "license": "MIT",
  "dependencies": {
    "amdefine": "^1.0.0",
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-webpack-alias": "^2.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "bluebird": "^3.5.0",
    "browser-meshblu-http": "^6.6.0",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "coffee-loader": "^0.7.3",
    "component-ie": "^1.0.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.27.1",
    "css-modules-require-hook": "^4.0.1",
    "debug": "^2.6.2",
    "eventemitter2": "^3.0.2",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.10.1",
    "filesize": "^3.5.6",
    "gzip-size": "^3.0.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "images": "^3.0.0",
    "lodash": "^4.15.0",
    "meshblu-firehose-socket.io": "^3.0.1",
    "moment": "^2.14.1",
    "npm-install-webpack-plugin": "^4.0.4",
    "postcss-loader": "^1.3.3",
    "rc-qrcode": "^1.0.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-flexbox": "^3.1.1",
    "react-router": "^3.0.2",
    "react-speech": "^0.3.1",
    "recursive-readdir": "^2.1.1",
    "rimraf": "^2.6.1",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.2",
    "superagent": "^3.5.0",
    "superagent-no-cache": "^0.1.1",
    "uuid": "^3.0.1",
    "webpack": "^2.2.1",
    "zooid-button": "^1.2.1",
    "zooid-error-state": "^1.3.0",
    "zooid-heading": "^1.0.8",
